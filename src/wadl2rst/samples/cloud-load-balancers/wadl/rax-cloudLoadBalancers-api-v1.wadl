    <method id="deleteCertificateMapping" name="DELETE">
        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN"
            title="Delete certificate mapping">
            <para role="shortdesc">
                Deletes a certificate mapping from a
                specified load balancer.
