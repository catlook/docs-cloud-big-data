    <method id="enableConnectionLogging" name="PUT">
        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN"
            title="Enable or disable connection logging">
            <para role="shortdesc">Enables or disables connection
                logging.</para>
        </wadl:doc>
        <request>
            <representation mediaType="application/json">
                <wadl:doc xmlns="http://docbook.org/ns/docbook"
                    xml:lang="EN">
                    <xsdxt:code
                        href="../samples/lb-enable-logging-request.json"
                    />
                </wadl:doc>
            </representation>
            <representation mediaType="application/xml" xml:lang="EN" element="lbapi:connectionLogging">
