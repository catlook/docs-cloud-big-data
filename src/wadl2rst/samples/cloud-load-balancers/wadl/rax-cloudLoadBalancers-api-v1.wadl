    <!-- contentcaching -->
    
    <method id="showContentCaching" name="GET">
        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN"
            title="Show content caching configuration">
            <para role="shortdesc">Shows the current configuration of
                content caching.</para>
            <para>This operation enables the user to view the current
                content caching configuration, enable content caching,
                or disable content caching.</para>
            <para>When content caching is enabled, recently-accessed
                files are stored on the load balancer for easy
                retrieval by web clients. Content caching improves the
                performance of high traffic web sites by temporarily
                storing data that was recently accessed. While it's
                cached, requests for that data are served by the load
                balancer, which in turn reduces load off the back end
                nodes. The result is improved response times for those
                requests and less load on the web server.</para>
            <para>For more information about content caching, see the
                following Knowledge Center article: <link
                    xlink:href="http://www.rackspace.com/knowledge_center/content/content-caching-cloud-load-balancers"
                    >Content Caching for Cloud Load
                Balancers</link>.</para>
        </wadl:doc>
        <response status="200">
            <representation mediaType="application/json">
                <wadl:doc xmlns="http://docbook.org/ns/docbook"
                    xml:lang="EN">
                    <xsdxt:code
                        href="../samples/lb-list-content-caching-response.json"
                    />
                </wadl:doc>
            </representation>
            <representation mediaType="application/xml" xml:lang="EN" element="lbapi:contentCaching">
                <wadl:doc xmlns="http://docbook.org/ns/docbook"
                    xml:lang="EN">
                    <xsdxt:code
                        href="../samples/lb-list-content-caching-response.xml"
                    />
                </wadl:doc>
            </representation>
        </response> &commonFaults; &getFaults; </method>
    
